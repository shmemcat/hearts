{
   "extends": "next/core-web-vitals",
   "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true
      }
   },
   "plugins": [
      "react"
   ],
   "rules": {
      "no-unused-vars": "off",
      "react/no-unescaped-entities": "off",
      "quotes": [
         2,
         "double"
      ]
   }
}